# Application Environment Variables
# Copy this file to .env and fill in the values
# Variables must be prefixed with APP__ to be loaded by the configuration system

# Environment: dev, prod
APP__DEV_ENV=dev

# LLM API Keys
APP__OPENAI_API_KEY=
APP__ANTHROPIC_API_KEY=
APP__GROQ_API_KEY=
APP__PERPLEXITY_API_KEY=
APP__GEMINI_API_KEY=

# Examples of nested configuration overrides
# APP__LOGGING__VERBOSE=true
# APP__LLM_CONFIG__CACHE_ENABLED=true
# APP__DEFAULT_LLM__DEFAULT_TEMPERATURE=0.7
